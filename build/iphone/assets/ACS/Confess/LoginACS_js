exports.LoginACS = function(){
	  //ログインの際、きちんと確認を行う
	  if(!Ti.App.Properties.hasProperty('username'))
	  	return false;
	  if(!Ti.App.Properties.hasProperty('password'))
	  	return false;
	  	
	  
	  require('/ACS/LoginACS').LoginACS(Ti.App.Properties.getString('username'),Ti.App.Properties.getString('password'));
	  
	 
}
